# EmailJS Configuration
# Get these values from https://www.emailjs.com/
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# ============================================================================
# Google AI APIs
# ============================================================================

# Gemini API Key (for Gemini Flash and general AI)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Med-Gemma API Key (if separate from Gemini)
VITE_MEDGEMMA_API_KEY=your_medgemma_api_key_here

# Google Astra API Key (if separate from Gemini)
VITE_ASTRA_API_KEY=your_astra_api_key_here

# ============================================================================
# Voice Synthesis (Daniel's Voice)
# ============================================================================

# GPT-SoVITS Voice Server URL
VITE_VOICE_SERVER_URL=/sovits

# Use remote voice server (true) or local browser TTS (false)
VITE_USE_REMOTE_VOICE=false

# ============================================================================
# Ecosystem Integration
# ============================================================================

# Polaris (Authentication)
VITE_POLARIS_URL=https://polaris-core.example.com

# Sirius (SafeCore - Security)
VITE_SIRIUS_URL=https://sirius-safecore.example.com

# Vega (DataCore - Storage)
VITE_VEGA_URL=https://vega-datacore.example.com

# ============================================================================
# Google Maps API (for mobile unit tracking)
# ============================================================================

VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# ============================================================================
# Client Customization
# ============================================================================

# Client logo URL (optional)
VITE_CLIENT_LOGO_URL=

# Client name
VITE_CLIENT_NAME=

# Client primary color
VITE_CLIENT_PRIMARY_COLOR=#00E5FF

# Client secondary color
VITE_CLIENT_SECONDARY_COLOR=#A855F7

# ============================================================================
# Feature Flags
# ============================================================================

# Enable inventory alerts
VITE_ENABLE_INVENTORY_ALERTS=false

# Enable telemedicine
VITE_ENABLE_TELEMEDICINE=false

# Enable wearables integration
VITE_ENABLE_WEARABLES=false

# Enable predictive analytics
VITE_ENABLE_PREDICTIVE_ANALYTICS=false
